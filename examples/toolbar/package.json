{
  "name": "toolbar",
  "description": "Shared toolbar component for managing document keys in @localfirst/state examples.",
  "version": "1.0.0",
  "license": "MIT",
  "private": false,
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "run-p build:*",
    "build:ts": "tsc",
    "build:css": "postcss src/styles.postcss -o dist/styles.css",
    "lint": "tsc -p tsconfig.strict.json --noEmit",
    "watch": "run-p watch:*",
    "watch:ts": "yarn build:ts --watch",
    "watch:css": "yarn build:css --watch",
    "test": "jest"
  },
  "dependencies": {
    "@emotion/core": "10",
    "taco-js": "0",
    "@localfirst/state": "1",
    "formik": "^2.0.1-rc.13",
    "friendly-words": "1",
    "react": "16",
    "react-dom": "^16.0.1",
    "redux": "4",
    "tailwindcss": "^1.7.6",
    "use-persisted-state": "0",
    "use-query-params": "0"
  },
  "devDependencies": {
    "@types/jest": "25",
    "@types/query-string": "6",
    "@types/use-persisted-state": "0",
    "jest": "25",
    "postcss-cli": "^7.1.2",
    "prettier": "2",
    "rimraf": "3",
    "ts-jest": "25",
    "tslib": "1",
    "typescript": "4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist"
    ]
  },
  "gitHead": "061c22c8e2481fac0ae5bbb70880b11e80c6f31e"
}

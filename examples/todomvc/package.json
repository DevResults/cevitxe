    "start": "node scripts/start.js"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "dependencies": {
    "@emotion/core": "^10.0.10",
    "cevitxe": "^0.1.0",
    "classnames": "2.2.6",
    "debug": "^4.1.1",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",

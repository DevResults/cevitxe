    "redux": "4.0.1",
    "resolve": "1.6.0",
    "uuid": "^3.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "@testing-library/cypress": "^4.0.2",
    "@types/classnames": "^2.2.8",
    "@types/debug": "^4.1.4",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/jest": "^24.0.13",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/uuid": "^3.4.4",
    "autoprefixer": "7.1.6",
